<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="personal.edu.trading">
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".EasyShopAppliction"
        android:allowBackup="true"
        android:icon="@mipmap/ic_lr"
        android:label="@string/app_name"
        tools:replace="android:icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.Activity_Guide">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.Activity_MeShop"/>
        <activity android:name=".activity.MainActivity" android:launchMode="singleTask"/>
        <activity android:name=".activity.Activity_Login"/>
        <activity android:name=".activity.Activity_Registered"/>
        <activity android:name=".activity.Activity_Message"/>
        <activity android:name=".activity.Activity_Upload"/>

    </application>

</manifest>